{
    "synch": [
        {
            "name": "filmz",
            "databases": ["dvdrental", "SampleCollections"],
            "tables": [
                {
                    "names": ["dvdrental.film", "SampleCollections.film"],
                    "keys": ["film_id", "_id"],
                    "connection": {
                        "type": "external_id_columns",
                        "columns": ["extern_id", "ext_id"]
                    },
                    "vectors": [
                        "title => title"
                    ]
                }
            ]
        },
        {
            "name": "test2",
            "databases": ["dvdrental", "SampleCollections"],
            "tables": [
                {
                    "names": ["dvdrental.film", "SampleCollections.film"],
                    "keys": ["film_id", "_id"],
                    "connection": {
                        "type": "unifier_persistence",
                        "initial_vector": "dvdrental <= SampleCollections"
                    },
                    "vectors": [
